/*
* Header and Nav 
*/

/*hamburger-nav*/

#header-main {
    
    float: left;
    width: 25%;
   /* width: 100%;*/
    height:  4.6em;
    padding-left: 3%;
    background-color: /*#f3f2f2;*/ transparent;

   /* overflow: auto;*/
     /*transparent*/
    /*background-color: rgba(0,0,0,.5)*/
    
    /*box-shadow:  0 2px 10px 0 rgba(0,0,0,.1);*/
}

.logo {
    float: left;
    width: 25%;
    padding: 0.4em 0 0 0;
}

.logo a {
    color:   /*#81c752;*/  #50872c;
    letter-spacing: 0;
    font-size: 1.5em;
    font-weight: 600;
    font-family: 'GeosansLight', Arial, sans-serif;
}

 nav {
      font-family: 'GeosansLight', Arial, sans-serif;
     
     float: right;    
     width: 75%;
     height: 4.6em;
     text-align: right;
     padding: 0 3% 0 0;
     background-color: /*#f3f2f2;*/ transparent;  
}


 nav ul {
    background-color: /*#f3f2f2;*/ transparent;
    letter-spacing: 0.2px;
    color:  #404040; 
     margin: 0;
     
    /*transparent*/
    /*background-color: rgba(0,0,0,.5)*/
}

 nav ul li a {
    background-color: /*#f3f2f2;*/ transparent;
    display: block;
    padding: 2em 1.7em;
    color: inherit;
    font-weight: 600;
    font-size: 0.9em;
    letter-spacing: 1.4px;
    text-transform: uppercase;
}

nav a:visited {
    color:  rgba(0,0,0,.9);
    text-decoration: none;
    transition: all 150ms ease-in;
}

/*obs!!!!!*/
nav .current-menu-item > a {
    color: /*#7dbf40;*/  #81c752; /*#78aa22;*/
    transition: all 150ms ease-in;
}


nav a:hover {
    color:  #81c752;  /*#78aa22;*/

    text-decoration: none;
    transition: all 150ms ease-in;
}


nav ul li {
    display: inline-block;
    position: relative; /*obs!*/
}

/*för att få undermenyn att ploppa ut och ligga till vänster*/
nav ul li ul {
    display: none;
    text-align: left; /*obs!*/
}

/*hover över li-element, undermenyn visas*/
nav ul li:hover ul {
    display: block;
    position: absolute;
    left: 0;
    width: 100%;
    padding: 0;
    top: 3.7em;
    background-color: /*#f3f2f2;*/ #fff;

    /*transparent*/
    /*background-color: rgba(0,0,0,.5);*/
    /* background-color: #404040; */
}

/*undermenyer under varann*/
nav ul li ul li {
    display: block;

}

/*höjd och hover-padding på undermenyerna*/
nav ul li ul a {
    padding:  0.6em 1.45em;
    background-color: /*#f3f2f2; */ transparent;
    letter-spacing: 1.6px;
    font-size: 0.95em;
    text-transform: lowercase;
    font-weight: 600;
}

.handle {  /*hamburgaren*/
    float: right;
    width: 25%;
    /*background-color: #404040;*/
    text-align: right;
    box-sizing: border-box;
    padding: 0.7em 1em;
    cursor: pointer;
    display: none;  /*obs!!*/
    color: #333;
    font-size: 1.7em;
    font-weight: 600;
}


