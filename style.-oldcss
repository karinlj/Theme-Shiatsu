/*buttons*/
.btn {
    font-family: Poppins, sans-serif;
    letter-spacing: .1em;
    text-transform: uppercase;
    height: 40px;
    line-height: 40px;
    font-size: 12px;
    padding: 0 30px;
    -webkit-transition: all .4s ease-in-out;
    transition: all .4s ease-in-out;
}

.btn-rounded {
    border-radius: 30px;
}

.btn-black {
    background: #2b2c2d;
    color: #fff;
}

.btn-black:hover{
    background: #fff;
    color: #2b2c2d;
}

.btn-white {
    background: #fff;
    color:#7BBF6A;
}

.btn-white:hover {
    background: #7BBF6A;
    color: #fff;
}

.btn-green {
    background: #7BBF6A;
    color: #fff;
}

.btn-green:hover {
    background: #fff;
    color: #7BBF6A;
}

.btn-border {
    background: #fff;
    color: #333;
    border: 1px solid #999;
}

.btn-border:hover {
    background: #fff;
    color: #333;
    border: none;
}

