/**********Media Queries*********/
@media (max-width: 992px) {  /* 960px*/
    
    
     .content-container {
        width: 80%;
    }
    
    .content-container-full-width {
        width: 80%;
    }
    
    nav ul li a {
        padding: 2.2em 0.9em;
    }
    
     .logo a {
        font-size: 1.15em;
    }
    
    
    /*testing*/
    .img-text {          
       padding: 15% 3em 0em 3em;
    }
    
    .image-boxes {
        margin: 4em auto 3em auto;
        width: 100%; 
    }
    
    .image-box .textwidget {
        padding: 0 20%;
        margin-bottom: 3em;
    }
    
    .intro-text {
        width: 90%;
        margin: 0 auto;
    }
    
    /*.box-padding img {
        padding: 0 5em;
    }*/
    
    .two-columns img {
        margin: 0;
        padding-top: 2em;
    }
    
    .three-columns img {
        width: 50%;
        padding-left: 0;
        padding-bottom: 3em;
    }
    
    .to-the-left {
        float: none;
        margin-left: 0;

    }
    
    .to-the-right {
        float: none;
        margin-right: 0;

    }
    
    .to-top {
        padding-bottom: 1em;
    }
}

@media (max-width: 768px) {
    
    .page-layout-image.banner {
        height: 260px;
        padding: 80px 0;
    }
    
    .front-page-layout.big {
         height: 460px;
